{
	"name": "hello-world-netlify-functions--parcel2",
	"description": "Test of Netlify functions with parcel2",
	"version": "0.0.1",
	"author": "Offirmo <offirmo.net@gmail.com> (https://www.offirmo.net/)",
	"private": true,


	"COMMENTS": {
		"01": "dependencies: Netlify build only installs PROD dependencies, so make sure modules critical for scripts are included!"
	},


	"PARCEL-CONFIG--BEGIN": "",
	"targets": {

		"frontend": {
			"context": "browser",
			"engines": {
				"browsers": "Chrome 100"
			},
			"outputFormat": "esmodule",
			"source": "src/web/index.html"
		},

		"backend--functions--hello-world": {
			"context": "node",
			"engines": {
				"node": ">=16"
			},
			"isLibrary": true,
			"outputFormat": "commonjs",
			"source": "src/functions/hello-world.mts",
			"includeNodeModules": {},
			"distDir": "./xyz/functions",
			"sourceMap": false
		},

		"backend--functions--hello-world-alt": {
			"context": "node",
			"engines": {
				"node": ">=16"
			},
			"isLibrary": true,
			"outputFormat": "commonjs",
			"source": "src/functions/hello-world-alt.mts",
			"includeNodeModules": {},
			"distDir": "./xyz/functions",
			"sourceMap": false
		}
	},
	"PARCEL-CONFIG--END": "",


	"scripts": {
		"clean": "rm -rf .parcel-cache ./xyz/functions/*",

		"_parcel--watch": "parcel watch",
		"_parcel--serve": "parcel --lazy",

		"_serve_functions": "netlify functions:serve",

		"build": "parcel build"
	},
	"devDependencies": {
		"@parcel/transformer-typescript-tsc": "^2",
		"@size-limit/file": "^7",
		"@types/aws-lambda": "^8",
		"npm-run-all": "^4",
		"parcel": "^2",
		"size-limit": "^7",
		"tslib": "^2",
		"typescript": "^4",
		"typescript-string-enums": "^1"
	},
	"repository": "https://github.com/Offirmo/hello-world-netlify-functions--parcel2.git"
}
